{
  "name": "Ciudad Capital",
  "host": "muds.maldorne.org",
  "port": "5010",
  "debug": false,
  "width": 800,
  "top": 0,
  "left": 0,
  "clean": false,
  "solo": false,
  "nocenter": false,
  "notrack": false,
  "nodrag": false,
  "embed": false,
  "separator": ";",
  "proxy": "wss://play.maldorne.org:6200/",
  "uncompressed": false,
  "useMuProtocol": false,
  "chatterbox": true,
  "chatterboxConfig": {
    "title": "Communications",
    "css": {
      "width": 400,
      "height": 400,
      "top": 0,
      "left": 800
    },
    "tabs": [
      {
        "name": "Creators",
        "channels": [
          "cre"
        ],
        "match": "\\[(Cre)\\] ([^:]+): (.+)",
        "replace": "<span class=\"creator\">[<span style=\"color:#FFD700\">$1</span>] <span style=\"color:#00CED1\">$2</span>: $3</span>",
        "time": true,
        "scroll": true
      },
      {
        "name": "Support",
        "channels": [
          "support"
        ],
        "match": "\\[(Support)\\] ([^:]+): (.+)",
        "replace": "<span class=\"support\">[<span style=\"color:#FF69B4\">$1</span>] <span style=\"color:#4169E1\">$2</span>: $3</span>",
        "time": true,
        "scroll": true
      },
      {
        "name": "Say",
        "channels": [
          "say"
        ],
        "match": "^(\\w+) (says) '([^']+)'$",
        "replace": "<span style=\"color:yellow\">$1</span> says '$2'",
        "time": true,
        "scroll": true
      },
      {
        "name": "Tell",
        "channels": [
          "tell"
        ],
        "match": "^(\\w+) (tells you) '([^']+)'$",
        "replace": "<span style=\"color:cyan\">$1</span> tells you '$2'",
        "time": true,
        "scroll": true
      }
    ]
  },
  "controlPanel": false,
  "groupTab": false,
  "initialIFrame": {
    "URL": "https://www.maldorne.org",
    "title": "Asociaci√≥n Maldorne"
  }
}